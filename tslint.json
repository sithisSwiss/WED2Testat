{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ],
    "max-classes-per-file": [false],
    "no-console": [false],
    "await-promise": true,
    "comment-format": false,
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}